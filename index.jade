							//- picture profile
							.col-md-5
								div(style="height:100%")
									img(src="images/profile.jpg" alt="profile" style="height:100%;margin: 0 12 0 12;")
							//- detail profile
							.col-md-7
								div(style="margin: 14px auto;")
