			.tessadf
				//- side bar
				div.col-md-2
					.list-group
						#accordion.panel-group(role='tablist', aria-multiselectable='true')
						.panel.panel-default
							#headingOne.panel-heading(role='tab')
								h4.panel-title
									a.collapsed(role='tab', data-toggle='collapse', data-parent='#accordion', href='#collapseOne', aria-expanded='true', aria-controls='collapseOne')
										| Master Data
							#collapseOne.panel-collapse.collapse.in(role='tabpanel', aria-labelledby='headingOne')
								.panel-body
									a(href="Supplier.html")
